<div class="section project" style="width: 100%; height:100%;">
	<div class="project_slider" id="products_slider" style="position: relative; top: 0px; left: 0px; overflow: hidden;">
	    <div class="project_slides" u="slides" style="cursor: move; position: absolute; overflow: hidden; left: 0px; top: 0px;">
	    	{{#each products}}
	        <div id="product_{{this.count}}" data-product="{{this.id}}" data-weight="{{this.weight}}" data-height="{{this.height}}" data-width="{{this.width}}" data-length="{{this.length}}" data-title="{{this.title}}" data-price="{{this.price}}" style="background-image:url({{this.image}});background-size:100% auto;background-repeat:no-repeat"></div>
	        {{/each}}
	    </div>
	</div>
</div>